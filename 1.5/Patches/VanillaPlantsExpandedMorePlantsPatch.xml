<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded - More Plants</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<AlphaCrafts.FruitOffsets>
							<defName>AC_FruitOffsets_MorePlants</defName>
							<fruitAndOffsetList>
								<li>
									<key>VCE_RawAvocado</key>
									<value>5</value>
								</li>
								<li>
									<key>VCE_RawMelon</key>
									<value>5</value>
								</li>
								<li>
									<key>VCE_RawPineapple</key>
									<value>2</value>
								</li>
								<li>
									<key>VCE_Mangoes</key>
									<value>3</value>
								</li>
								<li>
									<key>VCE_RawCoconut</key>
									<value>6</value>
								</li>
								<li>
									<key>VCE_RawAlmonds</key>
									<value>6</value>
								</li>
							</fruitAndOffsetList>
						</AlphaCrafts.FruitOffsets>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

