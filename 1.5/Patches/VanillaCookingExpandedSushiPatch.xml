<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Cooking Expanded - Sushi</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<AlphaCrafts.OilOffsets>
							<defName>AC_OilOffsets_Sushi</defName>
							<ingredientsAndOffsetList>
								<li>
									<key>VCE_RawSoybean</key>
									<value>3</value>
								</li>
								
							</ingredientsAndOffsetList>
						</AlphaCrafts.OilOffsets>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="AC_PressOil"]/ingredients/li/filter/thingDefs</xpath>
					<value>
						<li>VCE_RawSoybean</li>
						
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="AC_Oil"]/comps/li[@Class="AlphaCrafts.CompProperties_LabelByIngredients"]/overrides</xpath>
					<value>
						<li>
							<key>VCE_RawSoybean</key>
							<value>Soybean</value>
						</li>
						
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="AC_PressOil"]/fixedIngredientFilter/thingDefs</xpath>
					<value>
						<li>VCE_RawSoybean</li>
						
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="AC_PressOil"]/defaultIngredientFilter/thingDefs</xpath>
					<value>
						<li>VCE_RawSoybean</li>
						
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

