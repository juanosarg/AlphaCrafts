<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_PressOil"]/ingredients/li/filter/thingDefs</xpath>
		<value>
			<li>VCE_RawSoybean</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="AC_Oil"]/comps/li[@Class="AlphaCrafts.CompProperties_LabelByIngredients"]/overrides</xpath>
		<value>
			<li>
				<key>VCE_RawSoybean</key>
				<value>Soybean</value>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_PressOil"]/fixedIngredientFilter/thingDefs</xpath>
		<value>
			<li>VCE_RawSoybean</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_PressOil"]/defaultIngredientFilter/thingDefs</xpath>
		<value>
			<li>VCE_RawSoybean</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="AC_VinegarCask"]/comps/li[@Class="PipeSystem.CompProperties_AdvancedResourceProcessor"]/processes</xpath>
		<value>
			<li>AC_SoybeansToMiso</li>
			
		</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_PickleVegetables"]/ingredients/li[@Name="VegIngredient"]/filter/thingDefs</xpath>
		<value>
			<li>VCE_RawSoybean</li>
			
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_PickleVegetables"]/fixedIngredientFilter/thingDefs</xpath>
		<value>
			<li>VCE_RawSoybean</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_PickleVegetables"]/defaultIngredientFilter/thingDefs</xpath>
		<value>
			<li>VCE_RawSoybean</li>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_InfuseMayonnaise"]/ingredients/li[@Name="ProduceIngredient"]/filter/thingDefs</xpath>
		<value>
			<li>VCE_RawWasabi</li>
			
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_InfuseMayonnaise"]/fixedIngredientFilter/thingDefs</xpath>
		<value>
			<li>VCE_RawWasabi</li>
			
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/RecipeDef[defName="AC_InfuseMayonnaise"]/defaultIngredientFilter/thingDefs</xpath>
		<value>
			<li>VCE_RawWasabi</li>
			
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="AC_InfusedMayonnaise"]/comps/li[@Class="AlphaCrafts.CompProperties_LabelByIngredients"]/overrides</xpath>
		<value>
			<li>
				<key>VCE_RawWasabi</key>
				<value>Wasabi mayonnaise</value>
			</li>
			
		</value>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Cooking Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="AC_Miso"]/thingCategories</xpath>
					<value>
						<li>VCE_Condiments</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

