<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	
	<PipeSystem.ProcessDef>
		<defName>AC_CoconutsToVinegar</defName>
		<label>Ferment coconuts into vinegar</label>
		<description>Turns 50 coconuts into 25 vinegar.</description>
		<useIngredients>true</useIngredients>
		<spawnOnInteractionCell>true</spawnOnInteractionCell>
		<labelOverride>Ferment 50 coconuts into 25 vinegar</labelOverride>
		<ingredients>
			<li>
				<thing>VCE_RawCoconut</thing>
				<countNeeded>50</countNeeded>
			</li>
		</ingredients>
		<ticks>60000</ticks>
		<results>
			<li>
				<thing>AC_Vinegar</thing>
				<count>25</count>
			</li>
		</results>	
	</PipeSystem.ProcessDef>
	


</Defs>