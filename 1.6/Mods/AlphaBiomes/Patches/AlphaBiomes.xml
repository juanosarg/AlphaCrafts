			<li>AC_FrostLeavesIntoIcedElixir</li>
			
		</value>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Cooking Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<VEF.Graphics.GraphicOffsets>
							<defName>AC_JamOffsets_AB</defName>
							<thingDef>AC_Jam</thingDef>
							<ingredientsAndOffsetList>
								<li>
									<key>AB_RawTentacularFruit</key>
									<value>5</value>
								</li>
								<li>
									<key>AB_RawIashiphus</key>
									<value>1</value>
								</li>
								<li>
									<key>AB_RawSugarFamewort</key>
									<value>5</value>
								</li>
								<li>
									<key>AB_RawBloodBouquet</key>
									<value>1</value>
								</li>
								<li>
									<key>AB_RawRagadast</key>
									<value>2</value>
								</li>
							</ingredientsAndOffsetList>
						</VEF.Graphics.GraphicOffsets>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Brewing Expanded</li>
		</mods>
