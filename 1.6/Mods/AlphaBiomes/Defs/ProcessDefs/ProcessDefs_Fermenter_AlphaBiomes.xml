<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	
	<PipeSystem.ProcessDef>
		<defName>AC_FrostLeavesIntoIcedElixir</defName>
		<label>Ferment frost leaves into iced elixir</label>
		<description>Turns 150 frost leaves into 15 iced elixir.</description>
		
		<spawnOnInteractionCell>true</spawnOnInteractionCell>
		<labelOverride>Ferment 150 frost leaves into 15 iced elixir</labelOverride>
		<researchPrerequisites><li>AC_ArtisanalFermentation</li></researchPrerequisites>
		<ingredients>
			<li>
				<thing>AB_RawFrostLeaf</thing>
				<countNeeded>150</countNeeded>
			</li>
		</ingredients>
		<ticks>300000</ticks>
		<priorityInBillList>15</priorityInBillList>
		<results>
			<li>
				<thing>AC_IcedElixir</thing>
				<count>15</count>
			</li>
		</results>
		
	</PipeSystem.ProcessDef>

</Defs>