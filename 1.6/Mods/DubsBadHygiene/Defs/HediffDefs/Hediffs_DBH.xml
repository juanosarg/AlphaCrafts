<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<HediffDef>
		<defName>AC_SoapHediff</defName>
		<label>used soap</label>
		<makesSickThought>false</makesSickThought>
		<description>A fragrant soap has made this person squeaky-clean.</description>
		<tendable>false</tendable>
		<isBad>false</isBad>
		<defaultLabelColor>(92,62,198)</defaultLabelColor>
		<hediffClass>HediffWithComps</hediffClass>
		<initialSeverity>1.0</initialSeverity>
		<maxSeverity>1.0</maxSeverity>
		<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-1</severityPerDay>
				<showHoursToRecover>true</showHoursToRecover>
			</li>
		</comps>
		<stages>
			<li>
				<statFactors>
					<HygieneRateMultiplier>0.75</HygieneRateMultiplier>
				</statFactors>
			</li>
		</stages>
	</HediffDef>
	
	
</Defs>