<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	
	<PipeSystem.ProcessDef>
		<defName>AC_BlackTrufflesToTruffleOil</defName>
		<label>Infuse oil and black truffle into truffle oil</label>
		<description>Turns 15 oil and 15 black truffle into 6 truffle oil.</description>
		<useIngredients>true</useIngredients>
		<spawnOnInteractionCell>true</spawnOnInteractionCell>
		<labelOverride>Infuse 15 oil and 15 black truffle into 6 truffle oil</labelOverride>
		<priorityInBillList>5</priorityInBillList>
		<ingredients>
			<li>
				<thing>AC_Oil</thing>
				<countNeeded>15</countNeeded>
			</li>
			<li>
				<thing>AA_BlackTruffles</thing>
				<countNeeded>15</countNeeded>
			</li>
		</ingredients>
		<ticks>1200000</ticks>
		<results>
			<li>
				<thing>AC_TruffleOil</thing>
				<count>6</count>
			</li>
		</results>
		
	</PipeSystem.ProcessDef>
	

	

</Defs>