<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>AC_CraftRoyalSalve</defName>
		<label>craft royal salve</label>
		<description>Mix aloe vera salves with royal jelly to concentrate its healing properties.</description>
		<jobString>Crafting royal salve.</jobString>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>false</allowMixingIngredients>
		<workAmount>500</workAmount>
		<recipeUsers>
			<li>AC_ArtisanWorkshop</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>AC_AloeVeraSalve</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>VFEI2_RoyalInsectJelly</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>AC_AloeVeraSalve</li>
				<li>VFEI2_RoyalInsectJelly</li>
			</thingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
				<li>AC_AloeVeraSalve</li>
				<li>VFEI2_RoyalInsectJelly</li>
			</thingDefs>
		</defaultIngredientFilter>
		<workSkill>Cooking</workSkill>
		<products>
			<AC_RoyalSalve>10</AC_RoyalSalve>
		</products>
	</RecipeDef>
	
</Defs>
